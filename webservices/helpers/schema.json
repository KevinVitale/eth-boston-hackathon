{
  "definitions": {

  },
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "LOGN Schema",
  "required": [
    "creds",
    "address",
    "tokenID"
  ],
  "properties": {
    "creds": {
      "$id": "#/properties/creds",
      "type": "string",
      "title": "The Creds Schema",
      "default": "",
      "examples": [
        "andrew:washere"
      ],
      "pattern": "^(.*)$"
    },
    "address": {
      "$id": "#/properties/address",
      "type": "string",
      "title": "The Address Schema",
      "default": "",
      "examples": [
        "0x8347291305158bb6FE46757C7BDE7daFBD478456"
      ],
      "pattern": "^(.*)$"
    },
    "tokenID": {
      "$id": "#/properties/tokenID",
      "type": "integer",
      "title": "The Tokenid Schema",
      "default": 0,
      "examples": [
        1234
      ]
    }
  }
}